{
  "manifest_version": 3,
  "name": "Input Finder",
  "version": "1.0",
  "description": "Find and log input fields on a user's page",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "image.png"
  }
}
